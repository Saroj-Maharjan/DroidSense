CREATE TABLE CustomName (
    sessionUuid TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    deviceSerialNumber TEXT NOT NULL
);

getNameBy:
SELECT * FROM CustomName
WHERE sessionUuid = :sessionUuid;

names:
SELECT * FROM CustomName;

insert:
INSERT OR REPLACE INTO CustomName(sessionUuid, name, deviceSerialNumber)
VALUES(?,?, ?);

getNamesBySerialNumber:
SELECT * FROM CustomName
WHERE deviceSerialNumber = :deviceSerialNumber;

updateName:
UPDATE CustomName
SET name = :name
WHERE sessionUuid = :sessionUuid;

deleteBy:
DELETE FROM CustomName
WHERE sessionUuid = :sessionUuid;

nukeTable:
DELETE FROM CustomName;