CREATE TABLE CustomName (
    uuid TEXT NOT NULL PRIMARY KEY,
    name TEXT
);

getNameBy:
SELECT * FROM CustomName
WHERE uuid = :uuid;

insert:
INSERT OR REPLACE INTO CustomName(uuid, name)
VALUES(?,?);

updateName:
UPDATE CustomName
SET name = :name
WHERE uuid = :uuid;

deleteBy:
DELETE FROM CustomName
WHERE uuid = :uuid;

nukeTable:
DELETE FROM CustomName;